{
  "name": "back-anotaja",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "postbuild": "node scripts/fix-prisma-path.js",
    "build": "prisma generate && nest build",
    "prisma:generate": "prisma generate",
    "migrate:dev": "dotenv -e environments/.env.dev -- prisma migrate dev",
    "migrate:prod": "dotenv -e environments/.env.prod -- prisma migrate deploy",
    "prisma:generate:dev": "dotenv -e environments/.env.dev -- prisma generate",
    "prisma:generate:prod": "dotenv -e environments/.env.prod -- prisma generate",
    "build:dev": "dotenv -e environments/.env.dev -- npm run prisma:generate:dev && npm run prisma:db:push:dev && nest build",
    "build:prod": "dotenv -e environments/.env.prod -- npm run prisma:generate:prod && npm run prisma:db:push:prod && nest build",
    "start": "node -r tsconfig-paths/register dist/src/main.js",
    "start:prod": "node dist/src/main.js",
    "start:dev": "dotenv -e environments/.env.dev -- nodemon dist/src/main.js",
    "start:debug": "nest start --debug --watch",
    "prisma:reset:dev": "dotenv -e environments/.env.dev -- npx prisma migrate reset",
    "prisma:reset:prod": "dotenv -e environments/.env.prod -- npx prisma migrate reset",
    "prisma:db:push:dev": "dotenv -e environments/.env.dev -- npx prisma db push",
    "prisma:db:push:prod": "dotenv -e environments/.env.prod -- npx prisma db push",
    "db:seed": "tsx prisma/seed.ts",
    "db:seed:dev": "dotenv -e environments/.env.dev -- tsx prisma/seed.ts",
    "db:seed:prod": "dotenv -e environments/.env.prod -- tsx prisma/seed.ts",
    "db:seed-plans:dev": "dotenv -e environments/.env.dev -- tsx prisma/seed-plans.ts",
    "db:seed-plans:prod": "dotenv -e environments/.env.prod -- tsx prisma/seed-plans.ts",
    "prisma:push": "node scripts/prisma-push.js",
    "prisma:migrate": "dotenv -e .env -- prisma migrate dev",
    "prisma:studio": "dotenv -e .env -- prisma studio",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.980.0",
    "@aws-sdk/lib-storage": "^3.980.0",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/platform-socket.io": "^11.1.11",
    "@nestjs/schedule": "^6.1.0",
    "@nestjs/websockets": "^11.1.11",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "@types/multer": "^2.0.0",
    "@types/uuid": "^10.0.0",
    "@vendia/serverless-express": "^4.12.6",
    "aws-lambda": "^1.0.7",
    "axios": "^1.13.2",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "dotenv": "^17.2.3",
    "ioredis": "^5.9.2",
    "nodemailer": "^7.0.12",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "socket.io": "^4.8.3",
    "stripe": "^20.2.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/bcrypt": "^6.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.19.5",
    "@types/passport-jwt": "^4.0.1",
    "@types/pg": "^8.16.0",
    "@types/stripe": "^8.0.416",
    "@types/supertest": "^6.0.2",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^30.0.0",
    "nodemon": "^3.1.11",
    "prettier": "^3.4.2",
    "prisma": "^7.2.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.21.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
